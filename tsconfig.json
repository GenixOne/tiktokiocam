{
  "extends": "astro/tsconfigs/base",
  
  "compilerOptions": {
    "strictNullChecks": true,
    "allowJs": true,
    "outDir": "./dist/out-tsc",
    "baseUrl": "src",
    "paths": {
      "@lib/*": [
        "lib/*"
      ],
      "@utils/*": [
        "utils/*"
      ],
      "@components/*": [
        "components/*"
      ],
      "@layouts/*": [
        "layouts/*"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@pages/*": [
        "pages/*"
      ]
    },
    "types": [
      "vite-plugin-pwa/info"
    ],
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
